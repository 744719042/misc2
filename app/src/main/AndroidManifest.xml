<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.misc2">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DrawableActivity" />
        <activity android:name=".RippleActivity" />
        <activity android:name=".StrokeActivity" />
        <activity android:name=".LongPictureActivity" />
        <activity android:name=".DrawerActivity" />
        <activity android:name=".BezierActivity" />
        <activity android:name=".PathActivity" />
        <activity android:name=".TextAnimatorActivity" />
        <activity android:name=".LoadClassActivity" />
        <activity android:name=".BugFixActivity" />
    </application>

</manifest>